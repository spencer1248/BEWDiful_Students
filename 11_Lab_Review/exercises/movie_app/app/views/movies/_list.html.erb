<table border="1">
<tbody>
<tr>
<th> Movie Title</th>
<th> Movie Description</th>
<th> Movie Release Date</th>
<th> Movie Rating</th>
</tr>
<tr>
<% @movies.each do |movie| %>
	<td><%= link_to "#{movie.title}", movie_path(movie) %></td>
	<td><%= movie.description %></td>
	<td><%= movie.year_released %></td>
	<td><%= movie.rating %></td>
	</tr>
	<% end %>

	</tbody>
</table>